      },
      "tileFilter": {},
      "customName": "Journey",
      "query": "SELECT FUNNEL((useraction.name=\"Loading easyTravel\") OR (useraction.name=\"AppStart (easyTravel)\") AS \"Open easytravel\", useraction.name = \"DoLogin\" AS \"Log in\", useraction.name = \"searchJourney\" AS \"Search journey\", useraction.name = \"bookJourney\" AS \"Book journey\") FROM usersession WHERE useraction.application = \"${entity.mobile_application.name}\"",
      "type": "FUNNEL",
      "limit": 50
